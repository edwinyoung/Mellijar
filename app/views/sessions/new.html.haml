= provide(:title, "Sign in")
= stylesheet_link_tag "signin", media: "all"
= render "layouts/home_header"
= javascript_include_tag "signin"
#wrapper
  %a.btn{href: "/signup"} Sign Up
  #signin_container
    = form_for(:session, url: sessions_path) do |f|

      = f.text_field :email
      = f.password_field :password

      = f.submit "Sign in"
  = render "layouts/footer"
